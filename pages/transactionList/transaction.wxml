<view class="container"> 
  <view  wx:for='{{transactionList}}' wx:key="index" class="item" data-order-id="{{item.id}}" bindtap='openDetail'>
    <view class="left">
      <view class="type">{{item.orderType==1?'医生咨询':'视频购买'}}</view>
      <view class="time">{{item.orderTime}}</view>
    </view>
    <text>{{item.orderFee}}</text>
  </view>  
</view>