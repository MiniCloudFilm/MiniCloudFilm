<view class="container"> 
  <view class="docter-box" wx:for="{{counList}}" wx:key="index">
    <view class="weui-cell doctor-b" bindtap='trunConInterface' data-dialogid='{{item.dialogId}}' data-reportid="{{item.dialogReportId}}">
      <view class="weui-cell__hd doctorImg" style="position: relative;margin-right: 10px;">
        <image src="{{item.avatarUrl==null?'/image/icon-doctor.png':item.avatarUrl}}" style="width: 122rpx; height: 122rpx; display: block" />
        <view style="height:36px;" wx:if="{{userType=='2'}}"></view>
      </view>
      <view class="weui-cell__bd">
        <view class="doctorMes">
          <view class="weui-flex">
            <view class="weui-flex__item">
              <text style="font-weight:bold; font-size:30rpx;margin-right:20rpx; color:#2e3135;">{{item.receiverName}}</text>
              <text style="color:#1c7eff;margin-right:20rpx;">{{item.status==0?'未接受':(item.status==1?"咨询中":(item.status==2?'已结束':'已拒绝'))}}</text>
            </view>
            <view>
              <text>{{item.dialogTime}}</text>
            </view>
          </view>
          <view>
            <text>咨询的部位:</text>
            <text>{{item.bodyPart}}</text>
          </view>
        </view>
        <view class="weui-flex choice" wx:if="{{userTpye=='2'}}">
          <view class="accept">
            <button>接受</button>
          </view>
          <view class="refuse">
            <button>拒绝</button>
          </view>
        </view>
      </view>
    </view>
  </view>
</view>